{
  "name": "guardswift-server",
  "version": "5.1.9",
  "description": "GuardSwift server",
  "scripts": {
    "test": "mocha",
    "start": "./node_modules/.bin/ts-node index.ts",
    "dev": "nodemon",
    "clean": "npm run clean:cloud && npm run clean:shared && npm run clean:api",
    "clean:cloud": "find ./cloud -name \\*.js\\* -type f -delete",
    "clean:shared": "find ./shared -name \\*.js\\* -type f -delete",
    "clean:api": "find ./api -name \\*.js\\* -type f -delete"
  },
  "mocha": {
    "extension": ["ts"],
    "spec": "cloud/**/*.spec.ts",
    "require": "ts-node/register"
  },
  "dependencies": {
    "@parse/s3-files-adapter": "^1.2.1",
    "@sendgrid/mail": "^6.3.1",
    "@types/lodash": "^4.14.121",
    "@types/moment-timezone": "^0.5.10",
    "@types/node": "^11.10.4",
    "@types/parse": "^2.1.1",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.15",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "~4.16.x",
    "json2csv": "^4.3.5",
    "lodash": "^4.17.11",
    "lodash-move": "^1.1.1",
    "moment-holiday": "^1.5.1",
    "moment-timezone": "^0.5.23",
    "node-zip": "^1.1.1",
    "parse-server": "^3.1.3",
    "parse-server-push-adapter": "^2.0.2",
    "pdfmake": "^0.1.53",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "require-environment-variables": "^1.2.0",
    "require-glob": "^3.2.0",
    "ts-node": "^8.0.2",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3",
    "xlsx": "^0.14.1",
    "zlib": "^1.0.5"
  },
  "engines": {
    "node": "^8.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.18.10"
  }
}
